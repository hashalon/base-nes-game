/**
 * Handle palette
 */

import "src/nes";
import "src/define";
import "src/video";

namespace palette {
    
    in zeropage {

    }

    in program {

        const TEST : [u8] = [0x01, 0x16, 0x30];

        func load () {
            video.set_ppu_address(nes.ppu.ADDRESS_PALETTE_DATA + 1);
            video.write_array_in_ppu(TEST, 3);
        }
    }
}